    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express-session": "^1.17.2",
    "fast-csv": "^4.3.6",
    "ganache-cli": "^6.12.2",
    "googleapis": "^82.0.0",
    "handlebars": "^4.7.7",
    "moment": "^2.29.3",
    "mysql2": "^2.2.5",
    "node-mocks-http": "^1.10.1",
    "nodemailer": "^6.7.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pusher": "^5.1.0-beta",
    "rand-token": "^1.0.1",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
